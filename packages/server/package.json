{
  "name": "server",
  "version": "0.1.1",
  "description": "CasperLabs Explorer Server",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "~build": "tsc --watch",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon -e ts --watch src --exec \"npm run build && npm run start\"",
    "start:both": "concurrently \"npm run dev\" \"cd ../ui && npm start\"",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register test/**/*.test.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/auth0": "^2.20.5",
    "@types/chai": "^4.1.7",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "@types/request": "^2.48.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^6.1.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express-jwt": "0.0.42",
    "@types/http-proxy-middleware": "^0.19.3",
    "auth0": "^2.28.0",
    "blakejs": "^1.1.0",
    "casper-client-sdk": "^1.0.35",
    "command-line-args": "^5.1.1",
    "cron": "1.7.2",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "http-proxy-middleware": "^1.0.6",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.19",
    "minimist": ">=1.2.3",
    "prom-client": "^12.0.0",
    "request": "^2.88.2",
    "sdk": "^0.5.0",
    "tweetnacl-ts": "^1.0.3",
    "tweetnacl-util": "^0.15.0"
  }
