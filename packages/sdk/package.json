{
  "name": "casperlabs-sdk",
  "version": "0.9.1",
  "description": "SDK to interact with the CasperLabs blockchain using gRPC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn run clean && yarn run build && yarn run test",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "~build": "tsc --watch",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register test/**/*.test.ts",
    "docs": " typedoc --out docs src"
  },
  "keywords": [
    "CasperLabs"
  ],
  "author": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "minimist": ">=1.2.3",
    "mocha": "^6.1.4",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.4.1",
    "ts-protoc-gen": "^0.10.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.9.6",
    "@types/google-protobuf": "^3.7.1",
    "blakejs": "^1.1.0",
    "google-protobuf": "^3.8.0-rc.1",
    "jsbi": "^3.1.2",
    "rxjs": "^6.5.3",
    "tweetnacl-ts": "^1.0.3",
    "tweetnacl-util": "^0.15.0"
  }
}
